{
    "name": "react-starter-kit",
    "version": "1.0.0",
    "description": "react-starter-kit",
    "main": "src/index.jsx",
    "scripts": {
        "dev": "webpack-dev-server --mode=development",
        "build": "webpack-dev-server --mode=production",
        "flow": "flow",
        "analyze": "better-npm-run analyze",
        "eslint": "eslint --fix /src"
    },
    "betterScripts": {
        "analyze": {
            "command": "webpack-dev-server --mode=development",
            "env": {
                "ANALYZE": "true"
            }
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && flow check"
        }
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "eslint --fix"
        ],
        "*.{css,less}": [
            "stylelint --fix"
        ],
        "*.{html,json}": [
            "prettier --write"
        ],
        "*.{svg}": [
            "prettier --parser html --write"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AkhmerovM/react-starter-kit.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/AkhmerovM/react-starter-kit/issues"
    },
    "homepage": "https://github.com/AkhmerovM/react-starter-kit#readme",
    "devDependencies": {
        "@babel/core": "7.11.0",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-flow": "^7.10.4",
        "@babel/preset-react": "^7.10.4",
        "@svgr/webpack": "^5.4.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "better-npm-run": "^0.1.1",
        "clean-webpack-plugin": "^3.0.0",
        "core-js": "^3.6.5",
        "css-loader": "^4.2.1",
        "eslint": "^7.6.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-flowtype": "^5.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-react": "^7.20.6",
        "eslint-plugin-react-hooks": "^4.0.8",
        "file-loader": "^6.0.0",
        "flow-bin": "^0.131.0",
        "flow-typed": "^3.2.1",
        "html-webpack-plugin": "^4.3.0",
        "husky": "^4.2.5",
        "less-loader": "^6.2.0",
        "lint-staged": "^10.2.11",
        "mini-css-extract-plugin": "^0.10.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-loader": "^3.0.0",
        "prettier": "^2.0.5",
        "style-loader": "^1.2.1",
        "stylelint": "^13.6.1",
        "stylelint-config-standard": "^20.0.0",
        "terser-webpack-plugin": "^4.1.0",
        "url-loader": "^4.1.0",
        "webpack": "^4.44.1",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.1",
        "redux-thunk": "^2.3.0",
        "cssnano": "^4.1.10",
        "autoprefixer": "^9.8.6"
    }
}
